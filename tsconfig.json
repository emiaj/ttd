{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "moduleDetection": "force",
    },
    "exclude": ["node_modules"],
    "include": ["src"]
}